<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="596.0" prefWidth="1096.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="C868.ChooseUserToUpdateController">
    <children>
        <Label alignment="CENTER" layoutX="431.0" layoutY="21.0" prefHeight="30.0" prefWidth="201.0" text="Choose a User To Update" />
        <Button layoutX="962.0" layoutY="431.0" mnemonicParsing="false" onAction="#goToMainMenuWindow" text="Go To Main Menu" />

        <Button layoutX="18.0" layoutY="431.0" mnemonicParsing="false" onAction="#goToUpdateUserWindow" text="Update User" />
        <TableView fx:id="tableView" layoutX="18.0" layoutY="86.0" prefHeight="253.0" prefWidth="1059.0">
            <columns>
                <TableColumn fx:id="userIDCol" prefWidth="55.0" text="ID" />
