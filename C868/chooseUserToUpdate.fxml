                <TableColumn fx:id="passwordCol" prefWidth="159.0" text="Password" />
                <TableColumn fx:id="createDateCol" prefWidth="150.0" text="Created" />
                <TableColumn fx:id="createdByCol" prefWidth="147.0" text="Created By" />
                <TableColumn fx:id="lastUpdateDateCol" prefWidth="166.0" text="Last Update" />
                <TableColumn fx:id="lastUpdatedByCol" prefWidth="184.0" text="Last Update By" />

            </columns>
        </TableView>
        <Button layoutX="447.0" layoutY="437.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="30.0" prefWidth="170.0" text="Delete User" textFill="#e10808" />
        <Label fx:id="updateUserMessage" layoutX="214.0" layoutY="495.0" prefHeight="63.0" prefWidth="668.0" textFill="#e40e0e" />
    </children>

</AnchorPane>
